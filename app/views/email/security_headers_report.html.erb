        padding-bottom: 0 !important;
        color: #94a2aa;
        font-size: 15px;
        font-family: "Share Tech Mono", "Courier New", "Courier", monospace;
      }

      .rounded-image {
        border-radius: 55px;
        display: inline-block;
        vertical-align: middle;
        overflow: hidden;
      }

      .rounded-image img {
        float: none;
        display: block;
      }

      .link-github {
        font-family: "Share Tech Mono", "Courier New", "Courier", monospace;
        padding-left: 40px;
        vertical-align: middle;
        display: inline-block;
      }

      .link-github img {
        display: inline;
      }

      .link-github span {
        font-size: 40px;
      }

      .footer span {
        color: #202528 !important;
        font-size: 16px;
      }

      @media only screen and (max-width: 620.68966px) {
        table[class="body"] .container--full {
          width: 100% !important;
        }
      }
    </style>
    <!--[if mso]>
      <style>
        td {
          font-family: Arial, sans-serif !important;
        }

        td.outlook-monospace {
          font-family: 'Courier New', monospace !important;
        }

        .article .heading {
          font-family: 'Courier New', monospace !important;
          font-size: 20px;
        }

        .contacts td {
          font-family: 'Courier New', monospace !important;
        }

        a span {
          font-family: Arial, sans-serif !important;
        }
      </style>
    <![endif]-->
  </head>
  <body>
    <table class="body">
      <tr>
        <td align="center" valign="top" class="center">
          <center>
            <table class="container container--full">
              <tr>
                <td class="header-wrap">
                  <table class="row header">
                    <tr>
                      <td class="wrapper last">
                        <table class="twelve columns">
                          <tr>
                            <td class="three sub-columns header-logo"><img src="http://na-sjg.marketo.com/rs/srcclr/images/logo_small.png" alt="[:]"/></td>
                            <td class="nine sub-columns header-title last">Security Headers</td>
                            <td class="expander"></td>
                          </tr>
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
            <table class="container">
              <tr>
                <td>
                  <table class="row site-info">
                    <tr>
                      <td class="wrapper last">
                        <table class="twelve columns">
                          <tr>
                            <td class="six sub-columns site-name"><%= url %></td>
                            <td class="six sub-columns site-grade"> <span class="site-grade--label">(Overall grade)</span><span class="site-grade--value grade-<%= status.downcase %>"><%= status %></span></td>
                            <td class="expander"></td>
                          </tr>
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
            <table class="container">
              <tr>
                <td class="section-heading">
                  <table class="row">
                    <tr>
                      <td class="wrapper last">
                        <table class="twelve columns">
                          <tr>
                            <td class="six sub-columns test-section-name">
                              <h3 class="h3">HTTP headers</h3>
                            </td>
                            <td class="six sub-columns site-grade"><span class="site-grade--label">Grade:</span><span class="site-grade--value grade-<%= http_grade.downcase %>"><%= http_grade %></span></td>
                            <td class="expander"></td>
                          </tr>
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              <tr>
                <td class="tests-table">
                  <%- http_headers.each do |header| %>
                    <table class="row">
                      <tr>
                        <td class="wrapper">
                          <table class="four columns">
                            <tr>
                              <td><%= header[:label] %></td>
                              <td class="expander"></td>
                            </tr>
                          </table>
                        </td>
                        <td class="wrapper last">
                          <table class="eight columns">
                            <tr>
                              <td>
                                <span class="test-value <%= header[:status] %>"><%= header[:title] %></span>
                                <span class="test-value-description"><%= header[:description] %></span>
                              </td>
                              <td class="expander"></td>
                            </tr>
                          </table>
                        </td>
                      </tr>
                    </table>
                  <% end %>
                </td>
              </tr>
            </table>
            <table class="container">
              <tr>
                <td class="section-heading">
                  <table class="row">
                    <tr>
                      <td class="wrapper last">
                        <table class="twelve columns">
                          <tr>
                            <td class="six sub-columns test-section-name">
                              <h3 class="h3">CSP header</h3>
                            </td>
                            <td class="six sub-columns site-grade"><span class="site-grade--label">Grade:</span><span class="site-grade--value grade-<%= csp_grade.downcase %>"><%= csp_grade %></span></td>
                            <td class="expander"></td>
                          </tr>
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              <tr>
                <td class="tests-table">
                  <%- if csp_header[:directives].present? %>
                    <table class="row">
                      <tr>
                        <td class="wrapper last">
                          <table class="twelve columns">
                            <tr>
                              <td>
                                <p class="test-value">Content-Security-Policy:</p>
                                <%- csp_header[:directives].each do |directive| %>
                                  <p class="test-value-description"><%= directive %></p>
                                <% end %>
                              </td>
                              <td class="expander"></td>
                            </tr>
                          </table>
                        </td>
                      </tr>
                    </table>
                  <% end %>
                  <%- csp_header[:tests].each do |test| %>
                    <%- if test[:applicable?] %>
                      <table class="row">
                        <tr>
                          <td class="wrapper last">
                            <table class="twelve columns">
                              <tr>
                                <td>
                                  <p class="test-value <%= test[:status] %>"><%= test[:title] %></p>
                                  <span class="test-value-description"><%= test[:description] %></span>
                                </td>
                                <td class="expander"></td>
                              </tr>
                            </table>
                          </td>
                        </tr>
                      </table>
                    <% end %>
                  <% end %>
                </td>
              </tr>
            </table>
            <table class="container">
              <tr>
                <td>
                  <table class="row footer">
                    <tr>
                      <td class="wrapper last">
                        <table class="twelve columns">
                          <tr>
                            <td>You received this email because you have turned on notifications from <a href="<%= base_url %>/projects/security-headers">Security Headers Tool</a>. If you wish to unsubscribe or update your notification settings, please click <a href="<%= base_url %>/security-headers/my-sites">here</a>.</td>
                            <td class="expander"></td>
                          </tr>
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
          </center>
        </td>
      </tr>
    </table>
  </body>
</html>
